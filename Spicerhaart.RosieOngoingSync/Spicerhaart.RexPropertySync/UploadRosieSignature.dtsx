            {
                var sig = Dts.Variables["User::AutoAdSigPath"].Value + "Rosie\\" + Dts.Variables["$Package::PayrollNo"].Value + "\\" + "default.html";

                var fileStream = new FileStream(sig, FileMode.Open, FileAccess.Read);

                using (var streamReader = new StreamReader(fileStream, Encoding.UTF8))
                {
                    Dts.Variables["User::SignatureHTM"].Value = cleanForJSON(streamReader.ReadToEnd());
                }

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception)
            {
                Dts.Variables["User::SignatureHTM"].Value = "";
                Dts.TaskResult = (int)ScriptResults.Success;
            }
		}

